{
  "name": "dotenv-extractor",
  "displayName": "Dot Env Generator",
  "publisher": "DotEnvGen", 
  "description": "Extracts All process.env Variables Gloabally And Creates A .env File.",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extractEnvVars"
  ],
  "main": "./extension.js",
  "icon": "./Icon.png",
   "contributes": {
    "commands": [
      {
        "command": "extractEnvVars",
        "title": "Extract ENV Variables"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "pnpm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
